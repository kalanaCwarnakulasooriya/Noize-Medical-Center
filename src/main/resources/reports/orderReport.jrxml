	<query language="SQL"><![CDATA[SELECT 
    p.Name AS PatientName,
    o.OrderId,
    i.Name AS ItemName,
    od.Quantity,
    (od.Quantity * od.price) AS TotalAmount
FROM 
    patient p
JOIN 
    orders o ON p.PatientId = o.PatientId
JOIN 
    orderdetail od ON o.OrderId = od.OrderId
JOIN 
    item i ON od.ItemId = i.ItemId
ORDER BY 
    p.Name, o.OrderId
]]></query>
